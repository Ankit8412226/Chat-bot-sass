{
  "version": 2,
  "buildCommand": "npm run frontend:build",
  "outputDirectory": "frontend/dist",
  "installCommand": "npm ci && cd frontend && npm ci",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://backend.suhtech.shop/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
